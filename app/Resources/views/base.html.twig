<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}

<script type="text/javascript">
    jQuery(document).ready(function () {
        {% block documentready %}
        App.init();
        FormValidation.init();
        FormComponents.init();
        {% endblock %}
    });
</script>

<script type="text/javascript">
    var RecaptchaOptions = {
        theme: 'custom',
        custom_theme_widget: 'recaptcha_widget'
    };
</script>

</body>

</html>
