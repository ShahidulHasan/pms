    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>

    {% stylesheets filter='cssrewrite'
    '../app/Resources/plugins/font-awesome/css/font-awesome.min.css'
    '../app/Resources/plugins/bootstrap/css/bootstrap.min.css'
    '../app/Resources/plugins/uniform/css/uniform.default.css'

    '../app/Resources/plugins/bootstrap-wysihtml5/bootstrap-wysihtml5.css'
    '../app/Resources/plugins/fancybox/source/jquery.fancybox.css'

    '../app/Resources/plugins/jquery-file-upload/css/jquery.fileupload-ui.css'

    '../app/Resources/plugins/bootstrap-fileupload/bootstrap-fileupload.css'
    '../app/Resources/plugins/gritter/css/jquery.gritter.css'
    '../app/Resources/plugins/select2/select2_metro.css'
    '../app/Resources/plugins/clockface/css/clockface.css'
    '../app/Resources/plugins/bootstrap-datepicker/css/datepicker.css'
    '../app/Resources/plugins/jquery-multi-select/css/multi-select.css'
    '../app/Resources/plugins/bootstrap-switch/static/stylesheets/bootstrap-switch-metro.css'
    '../app/Resources/plugins/jquery-tags-input/jquery.tagsinput.css'
    '../app/Resources/plugins/bootstrap-markdown/css/bootstrap-markdown.min.css'
    '../app/Resources/css/plugins.css'

    '../app/Resources/css/style-metronic.css'
    '../app/Resources/css/style.css'
    '../app/Resources/css/style-responsive.css'
    '../app/Resources/css/themes/default.css'
    '../app/Resources/css/pages/inbox.css'
    '../app/Resources/css/custom.css' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
    <link rel="shortcut icon" href="favicon.ico"/>
</head>

<body class="page-header-fixed">

<div class="header navbar navbar-inverse navbar-fixed-top">
    <div class="header-inner">

        <a class="navbar-brand" href="index.html">
            {% image '../app/Resources/img/logo.png'
            filter='' output='/img/logo.png' %}
            <img src="{{ asset_url }}" alt="logo" class="img-responsive"/>
            {% endimage %}
        </a>

        <a href="javascript:;" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            {% image '../app/Resources/img/menu-toggler.png'
            filter='' output='/img/menu-toggler.png' %}
            <img src="{{ asset_url }}" alt="menu-toggler"/>
            {% endimage %}
        </a>

        <ul class="nav navbar-nav pull-right">
            <li class="dropdown user">
                <a href="index.html#" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown"
                   data-close-others="true">
                    {% image '../app/Resources/img/avatar1.png'
                    filter='' output='/img/avatar1.png' %}
                    <img src="{{ asset_url }}"/>
                    {% endimage %}
                    <span class="username">{{ app.user.username }}</span>
                    <i class="fa fa-angle-down"></i>
                </a>
                <ul class="dropdown-menu">
                    <li>
                        <a href="/profile"><i class="fa fa-user"></i> My Profile</a>
                    </li>
                    <li>
                        <a href="/logout"><i class="fa fa-key"></i> Log Out</a>
                    </li>
                </ul>
            </li>
        </ul>

    </div>
</div>

<div class="clearfix"></div>

<div class="page-container">

    <div class="page-sidebar navbar-collapse collapse">
        <ul class="page-sidebar-menu">
            <li>
                <div class="sidebar-toggler hidden-phone">
                    {% image '../app/Resources/img/sidebar-toggler.jpg'
                    filter='' output='/img/sidebar-toggler.jpg' %}
                    <img src="{{ asset_url }}"/>
                    {% endimage %}
                </div>
            </li>

            {% set Route, Dashboard, User, Client, Supplier, Product, File = app.request.attributes.get('_route'), [" "], [" "], [" "], [" "], [" "], [" "] %}
            {% set Url = Route|split('_') %}
            {% if Url[0] == 'homepage' %}{% set Dashboard = Dashboard|merge(["active"]) %}
            {% elseif Url[0] == 'fos' %}{% set Dashboard = Dashboard|merge(["active"]) %}
            {% elseif Url[0] == 'user' %}{% set User = User|merge(["active"]) %}
            {% elseif Url[0] == 'client' %}{% set Client = Client|merge(["active"]) %}
            {% endif %}

            <li class="{{ Dashboard|join(" ") }}">
                <a href="/">
                    <i class="fa fa-home"></i>
                    <span class="title"> Dashboard </span>
                    <span class="selected"></span>
                </a>
            </li>
            <li class="{{ User|join(" ") }}">
                <a href="/user/list">
                    <i class="fa fa-user"></i>
                    <span class="title"> User </span>
                    <span class="selected"></span>
                </a>
            </li>
            <li class="{{ Client|join(" ") }}">
                <a href="/client/list">
                    <i class="fa fa-user"></i>
                    <span class="title"> Client </span>
                    <span class="selected"></span>
                </a>
            </li>
                </a>
            </li>
        </ul>
    </div>

    <div class="page-content">
        <div class="row">
            <div class="col-md-12">
                {% block body %}{% endblock %}
            </div>
        </div>
    </div>

</div>

<div class="footer">
    <div class="footer-inner">
        2013 &copy; Create By RBS.
    </div>
    <div class="footer-tools">
        <span class="go-top">
            <i class="fa fa-angle-up"></i>
        </span>
    </div>
</div>

